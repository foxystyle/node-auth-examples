{
  env: {
    node: true,
    es6: true,
  },
  rules: {
    "semi": [1, "never"],
    "comma-dangle": [1, "always"],
    "no-func-assign": 1,
    "no-invalid-regexp": 1,
    "use-isnan": 2,
    "valid-typeof": 2,
    "default-case": 1,
    "dot-notation": 1,
    "eqeqeq": 1,
    "guard-for-in": 1,
    "no-alert": 1,
    "no-caller": 1,
    "no-eval": 1,
    "no-extra-bind": 1,
    "no-iterator": 1,
    "no-loop-func": 1,
    "no-multi-spaces": 1,
    "no-native-reassign": 1,
    "no-proto": 1,
    "no-redeclare": 1,
    "no-self-compare": 1,
    "no-void": 1,
    "wrap-iife": 1,
    "no-shadow": 1,
    "no-undef": 2,
    "no-undef-init": 1,
    "no-undefined": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "comma-spacing": 1,
    "max-nested-callbacks": [1, 4],
    "no-multiple-empty-lines": [1, {"max": 2}],
    "object-curly-spacing": [1, "always"],
    "keyword-spacing": 1,
    "spaced-comment": 1,
    "no-var": 1,
  }
}
